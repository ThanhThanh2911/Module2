@page
@model CompanyApp.Pages.Company.ListModel
@{
    ViewData["Title"] = "List";
}

<h2>List</h2>
<div class="container">
    <h2>Carousel Example</h2>
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#carouselExampleControls" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleControls" data-slide-to="1"></li>
            <li data-target="#carouselExampleControls" data-slide-to="2"></li>
        </ol>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="https://svmp-blog.s3.amazonaws.com/blog/wp-content/uploads/2019/03/dia-diem-chup-anh-dep-o-Bac-Ninh-canh-dong-co-lau-2.jpg" class="d-block w-100" alt="...">
                </div>
                <div class="carousel-item">
                    <img src="data:image/jpeg;base64," class="d-block w-100" alt="...">
                </div>
                <div class="carousel-item">
                    <img src="https://blogtraitim.info/wp-content/uploads/2018/06/Hinh-anh-dep-2.jpg" class="d-block w-100" alt="...">
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
</div>
