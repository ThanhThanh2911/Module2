@page 
@model TestMVC.Pages.Students.CreateModel
@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="Student.Name"></label>
        <input asp-for="Student.Name" />
    </div>

    <div class="form-group">
        <label asp-for="Student.Phone"></label>
        <input asp-for="Student.Phone" />
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
    <a asp-page="./List">Back List</a>
</form>

